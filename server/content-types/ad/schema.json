{
  "kind": "collectionType",
  "collectionName": "ads",
  "info": {
    "singularName": "ad",
    "pluralName": "ads",
    "displayName": "Ad"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "ad_name": {
      "type": "string"
    },
    "ad_id": {
      "type": "uid",
      "targetField": "ad_name"
    },
    "ad_status": {
      "type": "enumeration",
      "enum": [
        "draft",
        "live",
        "inactive",
        "expired",
        "archived"
      ],
      "default": "draft"
    },
    "ad_start_date": {
      "required": true,
      "type": "date"
    },
    "ad_end_date": {
      "type": "date"
    },
    "ad_type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::strapi-ads.ad-type"
    },
    "ad_headline": {
      "type": "string"
    },
    "ad_description": {
      "type": "text"
    },
    "ad_image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "ad_video_url": {
      "type": "string"
    },
    "ad_external_url": {
      "type": "text"
    },
    "ad_destination_models": {
      "type": "enumeration",
      "enum": [
        "general",
        "news"
      ]
    },
    "ad_destination_page": {
      "type": "string"
    },
    "ad_cta_name": {
      "type": "string"
    },
    "campaign": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::strapi-ads.campaign",
      "inversedBy": "ads"
    },
    "ad_spot": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::strapi-ads.ad-spot"
    },
    "ad_screens": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::strapi-ads.ad-screen"
    },
    "total_impressions": {
        "type": "biginteger"
    },
    "total_clicks": {
        "type": "biginteger"
    }
  }
}
