{
  "kind": "collectionType",
  "collectionName": "ad_types",
  "info": {
    "singularName": "ad-type",
    "pluralName": "ad-types",
    "displayName": "Ad Type",
    "description": ""
  },
  "options": { "draftAndPublish": true },
  "pluginOptions": {},
  "attributes": {
    "thumbnail": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": ["images"]
    },
    "title": { "type": "string" },
    "description": { "type": "text" },
    "allow_video": { "type": "boolean", "default": false },
    "ad_type_id": { "type": "uid", "targetField": "title" },
    "image_size": { "type": "json" },
    "ad_spots": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::strapi-ads.ad-spot",
      "mappedBy": "ad_types"
    }
  }
}
